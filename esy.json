{
  "name": "rarg-dev",
  "version": "0.1.0",
  "description": "Example Reason Esy Project",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build --profile=release",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "install": [
      "esy-installer rarg.install",
      "esy-installer tests.install",
      "esy-installer qw.install"
    ]
  },
  "scripts": {
    "test": "esy x bash -cx ./scripts/test-dev.cmd",
    "test-ci": "esy x bash -cx ./scripts/test-ci.cmd",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "@esy-ocaml/reason": ">= 3.4.0 < 3.5.0",
    "@opam/dune": "1.10.0",
    "@reason-native/console": "*",
    "@reason-native/pastel": "*",
    "@reason-native/rely": "*",
    "ocaml": "~4.6.0",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.6.0",
    "@opam/odoc": "*"
  }
}
